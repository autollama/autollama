{
  "name": "autollama-api",
  "version": "3.0.2",
  "description": "AutoLlama API Server - Production-ready RAG platform with contextual embeddings and intelligent document processing",
  "main": "server.js",
  "keywords": [
    "api",
    "rag",
    "llm",
    "embeddings",
    "vector-search",
    "openai",
    "document-processing",
    "contextual-embeddings",
    "postgresql",
    "qdrant"
  ],
  "homepage": "https://github.com/your-username/autollama#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/autollama.git",
    "directory": "api"
  },
  "bugs": {
    "url": "https://github.com/your-username/autollama/issues"
  },
  "license": "MIT",
  "author": {
    "name": "AutoLlama Team",
    "url": "https://github.com/your-username/autollama"
  },
  "scripts": {
    "start": "node migrate-docker.js --auto --docker && node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest src/tests/unit/",
    "test:integration": "jest src/tests/integration/",
    "test:e2e": "jest src/tests/e2e/ --config=jest.performance.config.js",
    "test:performance": "node --expose-gc ./node_modules/.bin/jest --config=jest.performance.config.js",
    "test:load": "node --expose-gc --max-old-space-size=2048 ./node_modules/.bin/jest src/tests/performance/load-testing.performance.test.js --config=jest.performance.config.js",
    "test:stress": "node --expose-gc --max-old-space-size=2048 ./node_modules/.bin/jest src/tests/performance/resource-monitoring.performance.test.js --config=jest.performance.config.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "build": "npm run lint && npm run test",
    "precommit": "npm run lint && npm run format:check && npm run test"
  },
  "dependencies": {
    "@postlight/mercury-parser": "^2.2.1",
    "axios": "^1.6.0",
    "better-sse": "^0.15.1",
    "bullmq": "^5.56.9",
    "busboy": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "color": "^5.0.0",
    "color-string": "^2.1.0",
    "connect-busboy": "^1.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "epub": "^1.2.0",
    "event-target-shim": "^6.0.2",
    "express": "^4.18.2",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "kuler": "^2.0.0",
    "mammoth": "^1.6.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.6.7",
    "node-html-markdown": "^1.3.0",
    "openai": "^4.20.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "raw-body": "^3.0.0",
    "simple-swizzle": "^0.2.2",
    "streamsearch": "^1.1.0",
    "swagger-ui-express": "^5.0.0",
    "text-hex": "^1.0.0",
    "tmp": "^0.2.1",
    "turndown": "^7.1.2",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "dezalgo": "^1.0.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "once": "^1.4.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "wrappy": "^1.0.2",
    "yallist": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
